  async function loadUsageDashboard() {
    try {
      const tenantId = window.currentTenant || 'apoyar'; // Default to apoyar for now
      const response = await fetch(window.location.protocol + '//' + window.location.host + `/api/tenant/${tenantId}/usage`, {
